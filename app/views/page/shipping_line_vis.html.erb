		function initialize() {
			var mapCanvas = document.getElementById('map');
			var styles = [
						{
						  stylers: [
						  	{ hue: "FFFFE3"},
						    { saturation: -90 }
						  ]
						}
					];

			var mapOptions = {
			  center: new google.maps.LatLng(12,120.6217543),
			  zoom: 6,
			  mapTypeId: google.maps.MapTypeId.ROADMAP
			}
			map = new google.maps.Map(mapCanvas, mapOptions)

					map.setOptions({styles: styles});

			var markerIconOp = {
			      path: google.maps.SymbolPath.CIRCLE,
			      scale: 2,
			      strokeWeight: 4,
			      strokeColor: "#D23439"
			      // strokeColor: "#4D91AD"
			    };

			R=initR(markerIconOp, map);
			paths=createLines(R);
		}
		google.maps.event.addDomListener(window, 'load', initialize);

		var showingLines = false;
		function toggleLines(){
			showingLines = !showingLines;
			showLines(paths, showingLines, map);
		}

		function createLines(R){
			var paths = [];
			for (i = 0; i < R.length; i++) { 
				if(!R[i]) continue;
			     var path = new google.maps.Polyline({
							  path:R[i],
							  strokeColor:"#D23439",
							  strokeOpacity:0.3,
							  strokeWeight:2,
							  geodesic: true
							});
			     paths.push(path);
			}
			return paths;
		};

		function showLines(paths, on, map){
			for (i = 0; i < paths.length; i++)
				paths[i].setMap(on ? map : null);
		};
