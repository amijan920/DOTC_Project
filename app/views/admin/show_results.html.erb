<%= render "admin/admin_navbar" %>

<script>
	var view = "configresults";
</script>
<div id="contents" class="container">
	<ul class="nav nav-tabs">
		<% @tables.each do |ind, table| %>
 			<li><a data-toggle="tab" class="nav-urls"><%= table[:url] %></a></li>
		<% end %>
	</ul>
	<br />

	<div class="tab-content">
		<% @tables.each do |ind, table| %>
		<div class="tab-pane fade">
			<table class='table table-striped table-responsive'>
				<% table[:lines].each_with_index do |line,i| %>
					<tr>
					<% if i==0 %>
						<% line.split(/\t/).each do |term| %>
							<th><%= term %></th>
						<% end %>
					<% else %>
						<% line.split(/\t/).each do |term| %>
							<td><%= term %></td>
						<% end %>
					<% end %>
					</tr>
				<% end %>
			</table>
		</div>
		<% end %>
	</div>
</div>
<script>
$('.tab-pane').each(function (index){
	if(index == 0){
		$(this).addClass('in');
		$(this).addClass('active');
	}

	$(this).attr('id', "tab-"+index);
});

$('.nav-urls').each(function (index){
	if(index == 0)
		$(this).parent().addClass('active');

	$(this).attr('href', "#tab-"+index);

	$(this).click(function (e) {
	  e.preventDefault()
	  $(this).tab('show')
	});
});
</script>