<div id="origin-name-container" class="panel-sub">
<div>
	<strong>Origin:</strong> Ninoy Aquino International Airport
</div>
</div>
<div id="destination-name-container" class="panel-sub">
<div>
	<strong>Destination:</strong> Mactan-Cebu International Airport
</div>
</div>
<div id="routes-container">
	<div id="routes-info">
		<div class="section-title">
			Suggested Routes
		</div>
		<div class="scroll-container">
			<div id="route-1" class="route-info panel-sub">
				<div class="bold">
					Direct
				</div>
				<div class="small-desc">
					<table class="route-table">
						<tr>
							<td>Departure Times:</td>
							<td align="right">4:00, 5:00, 7:00</td>
						</tr>
						<tr>
							<td>Average Travel Time:</td>
							<td align="right">1 hour 15 minutes</td>
						</tr>
					</table>
				</div>
				<div class="route-visualization">
				</div>
			</div>
			<div id="route-2" class="route-info panel-sub">
				<div class="bold">
					Direct
				</div>
				<div class="small-desc">
					<table class="route-table">
						<tr>
							<td>Departure Times:</td>
							<td align="right">4:00, 5:00, 7:00</td>
						</tr>
						<tr>
							<td>Average Travel Time:</td>
							<td align="right">4 hour 20 minutes</td>
						</tr>
					</table>
				</div>
				<div class="route-visualization">
				</div>
			</div>
		</div>
	</div>
</div>

<script id="routes-info-template" type="text/x-handlebars-template">
	{{#each info_list}}
	  <div class="add-info panel-sub">
			<div class="bold">
				{{name}}
			</div>
			<div class="small-desc">
				{{body}}
			</div>
		</div>
  {{/each}}
</script>