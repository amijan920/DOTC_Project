    	function shipChange(){
			var shippingLine = document.getElementById("shippingLines").value;
			if(showingLines){
				showingLines = false; 
				showLines(paths, false, map);
			}
			paths=createLines(shipLines[shippingLine]);

			if(!!shipLines[shippingLine][0]) 
				map.setCenter(new google.maps.LatLng(shipLines[shippingLine][0][0].H,shipLines[shippingLine][0][0].L));
			else
				map.setCenter(new google.maps.LatLng(shipLines[shippingLine][1][0].H,shipLines[shippingLine][1][0].L));
			map.setZoom(8);

			toggleLines();
    	}

